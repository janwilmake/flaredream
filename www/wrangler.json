  "compatibility_date": "2025-06-28",
  "dev": { "port": 3000 },
  "observability": { "logs": { "enabled": true } },
  "assets": { "directory": "./" },
  "routes": [
    { "custom_domain": true, "pattern": "flaredream.com" },
    { "custom_domain": true, "pattern": "www.flaredream.com" }
  ]
}
